<h1>Salut <%= current_user.username %> üñê</h1>
<section class="py-5 text-center container-fluid">
  <div class="row py-lg-5 backimg">
    <div class="col-lg-6 col-md-8 mx-auto">
      <h1 class="fw-light" style="color:white;">Photos</h1>
      <p class="lead text-muted" style="color:white;">Partages tes photos en cliquant sur ...</p>
    </div>
    <%= link_to 'New Photo', new_photo_path, class: "btn btn-sm btn-outline-secondary" %>
  </div>
</section>


<div class="d-flex flex-wrap">
  <% @photos.each do |photo| %>
    <div class="card-deck" style="width:35vw">
      <div class="col">
        <div class="card shadow-sm">
          <p class="card-text">
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="32" height="32" class="rounded-circle" viewBox="0 0 448 448">
              <image id="user" xlink:href="data:image/png;base64,"/>
            </svg>
            <%= photo.user.username %></p>
          <img src="<%= photo.url %>">
          <div class="card-body">
            <h2 class="card-text"><%= photo.title %></h2>
            <p class="card-text"><%= photo.description %></p>
            <p class="card-text"><%= photo.camera %></p>
            <div class="d-flex justify-content-between align-items-center">
              <p class="card-text"><%= photo.aperture %></p>
              <p class="card-text">F<%= photo.focal %></p>
              <p class="card-text"><%= photo.darkeningspeed %></p>
            </div>
            <p class="card-text">#<%= photo.tags %></p>
            <div class="d-flex justify-content-between align-items-center">
              <div class="btn-group">
                <%= link_to 'View', photo, class: "btn btn-sm btn-outline-secondary" %>
                <%= link_to 'Edit', edit_photo_path(photo), class: "btn btn-sm btn-outline-secondary" %>
                <%= link_to 'Destroy', photo, method: :delete, data: { confirm: 'Are you sure?' }, class: "btn btn-sm btn-outline-secondary" %>
            
              </div>
              <small class="text-muted"><%= photo.place %></small>
              <small class="text-muted"><%= photo.situation %></small>
            </div>
          </div>
        </div>
      </div>
    </div>
  <% end %>
</div>
<br>
<%= link_to 'New Photo', new_photo_path %>


